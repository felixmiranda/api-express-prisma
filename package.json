{
  "name": "api-express-prisma",
  "version": "1.0.0",
  "description": "API demo using express, prisma and postgre",
  "main": "index.js",
  "engines": {
    "node": "16.15.0",
    "npm": "8.5.5"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "generate-schema": "cat src/*/*.prisma > prisma/schema.prisma && npx prisma format",
    "postinstall": "tsc",
    "start": "node dist/server.js",
    "lint": "npx eslint . --ext .ts"
  },
  "keywords": [],
  "author": "Joseph Quispe",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "prisma": "^3.14.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.8.0"
  }
}
